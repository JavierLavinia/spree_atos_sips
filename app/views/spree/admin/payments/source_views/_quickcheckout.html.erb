<fieldset>
  <legend><%= t('atos') %></legend>

  <table class="index">
    <tr>
      <th colspan="6"><%= t('atos_transaction') %></th>
    </tr>
    <tr>
      <td><label><%= t("email") %>:</label></td>
      <td>
        <%= payment.source.email %>
      </td>
    </tr>
    <tr>
      <td><label><%= t("atos_customer_id") %>:</label></td>
      <td>
        <%= payment.source.customer_id %>
      </td>
    </tr>
    <tr>
      <td><label><%= t("atos_transaction_id") %>:</label></td>
      <td>
        <%= payment.source.transaction_id %>
      </td>
    </tr>
    <tr>
      <td><label><%= t("atos_payment_type") %>:</label></td>
      <td>
        <%= payment.source.payment_type %>
      </td>
    </tr>
    <tr>
      <td><label><%= t("atos_payment_currency") %>:</label></td>
      <td>
        <%= payment.source.currency %>
      </td>
    </tr>
  </table>
</fieldset>